{
  "name": "QQ 音乐",
  "version": "1.0.0",
  "platform": "qq",
  "description": "QQ 音乐搜索和播放（4个备选API，自动切换）",
  "author": "Music Free",
  "homepage": "https://music.qq.com",
  "apiServers": [
    "https://qq-music-api.vercel.app",
    "https://qqmusic-api.vercel.app",
    "https://api.music.qq.com",
    "https://u.y.qq.com"
  ],
  "searchApi": {
    "method": "GET",
    "url": "/search?keyword={keyword}&pagesize=30",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data",
    "itemMapping": {
      "id": "id",
      "title": "name",
      "artist": "singer[0].name",
      "album": "album.name",
      "duration": "duration",
      "cover": "album.mid"
    }
  },
  "playUrlApi": {
    "method": "GET",
    "url": "/url?id={id}&br=320000",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data.url"
  },
  "lyricApi": {
    "method": "GET",
    "url": "/lyric?id={id}",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "lrc"
  }
}
