{
  "name": "酷我音乐",
  "version": "1.0.0",
  "platform": "kuwo",
  "description": "酷我音乐搜索和播放（4个备选API，自动切换）",
  "author": "Music Free",
  "homepage": "https://www.kuwo.cn",
  "apiServers": [
    "https://kuwo-music-api.vercel.app",
    "https://www.kuwo.cn/api",
    "https://api.kuwo.cn",
    "https://music.kuwo.cn/api"
  ],
  "searchApi": {
    "method": "GET",
    "url": "/search?keyword={keyword}&pagesize=30",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data",
    "itemMapping": {
      "id": "id",
      "title": "name",
      "artist": "artist",
      "album": "album",
      "duration": "duration",
      "cover": "pic"
    }
  },
  "playUrlApi": {
    "method": "GET",
    "url": "/url?id={id}&rid={rid}",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data.url"
  },
  "lyricApi": {
    "method": "GET",
    "url": "/lyric?id={id}&rid={rid}",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data.lyric"
  }
}
