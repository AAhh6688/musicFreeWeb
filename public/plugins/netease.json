{
  "name": "网易云音乐",
  "version": "1.0.0",
  "platform": "netease",
  "description": "网易云音乐搜索和播放（4个备选API，自动切换）",
  "author": "Music Free",
  "homepage": "https://music.163.com",
  "apiServers": [
    "https://netease-cloud-music-api-ruby.vercel.app",
    "https://music-api.leanapp.cn",
    "https://netease-api.vercel.app",
    "https://api.netease.com"
  ],
  "searchApi": {
    "method": "GET",
    "url": "/search?keywords={keyword}&type=1&limit=30",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "result.songs",
    "itemMapping": {
      "id": "id",
      "title": "name",
      "artist": "artists[0].name",
      "album": "album.name",
      "duration": "duration",
      "cover": "album.picUrl"
    }
  },
  "playUrlApi": {
    "method": "GET",
    "url": "/song/url?id={id}&br=320000",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data[0].url"
  },
  "lyricApi": {
    "method": "GET",
    "url": "/lyric?id={id}",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "lrc.lyric"
  }
}
