{
  "name": "网易云音乐",
  "version": "1.0.0",
  "description": "网易云音乐搜索和播放",
  "author": "Music Free",
  "homepage": "https://music.163.com",
  "apis": [
    {
      "name": "搜索",
      "url": "https://netease-cloud-music-api-ruby.vercel.app/search?keywords={keyword}&type=1&limit=30",
      "method": "GET",
      "headers": {
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
      },
      "dataPath": "result.songs",
      "mapping": {
        "id": "id",
        "name": "name",
        "artist": "artists.0.name",
        "album": "album.name",
        "duration": "duration",
        "cover": "album.picUrl"
      }
    },
