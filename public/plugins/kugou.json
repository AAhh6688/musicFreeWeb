{
  "name": "酷狗音乐",
  "version": "1.0.0",
  "platform": "kugou",
  "description": "酷狗音乐搜索和播放（4个备选API，自动切换）",
  "author": "Music Free",
  "homepage": "https://www.kugou.com",
  "apiServers": [
    "https://kugoumusicapi.4everland.app",
    "https://kugou-music-api.vercel.app",
    "https://api.kugou.com",
    "https://music.kugou.com/api"
  ],
  "searchApi": {
    "method": "GET",
    "url": "/search?keyword={keyword}&pagesize=30",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data",
    "itemMapping": {
      "id": "id",
      "title": "songName",
      "artist": "singerName",
      "album": "albumName",
      "duration": "duration",
      "cover": "imgUrl"
    }
  },
  "playUrlApi": {
    "method": "GET",
    "url": "/url?songid={id}&hash={hash}",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data.url"
  },
  "lyricApi": {
    "method": "GET",
    "url": "/lyric?songid={id}&hash={hash}",
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "dataPath": "data.lyric"
  }
}
